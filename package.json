{
  "name": "ember-docgen",
  "description": "A CLI and toolbox to extract information from Ember component files for documentation generation purposes. ",
  "version": "0.0.5",
  "author": "Rajasegar Chandran @rajasegar",
  "bin": {
    "ember-docgen": "./bin/run"
  },
  "bugs": "https://github.com/rajasegar/ember-docgen/issues",
  "dependencies": {
    "@oclif/command": "^1.5.18",
    "@oclif/config": "^1.13.2",
    "@oclif/plugin-help": "^2.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/rajasegar/ember-docgen",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "ember-docgen",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "rajasegar/ember-docgen",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "deploy": "npm version patch && git push && git push --tags && npm publish"
  }
}
